{
  "name": "sqrtox-website",
  "version": "1.0.0",
  "description": "This is the site where I put my blog and other information.",
  "main": "./dist/server/index.js",
  "scripts": {
    "precompile": "rimraf ./dist/",
    "compile": "run-s compile:*",
    "compile:tsc": "tsc -p tsconfig.server.json",
    "compile:tsc-alias": "tsc-alias -p tsconfig.server.json",
    "clean": "rimraf ./.next/",
    "predev": "npm run clean",
    "dev": "run-s dev:*",
    "dev:compile": "npm run compile",
    "dev:start": "cross-env NODE_ENV=development node .",
    "prebuild": "npm run clean",
    "build": "run-s prod:*",
    "build:build": "cross-env NODE_ENV=production next build",
    "build:sitemap": "next-sitemap --config ./next-sitemap.config.mjs",
    "build:compile": "npm run compile",
    "vercel-build": "npm run build",
    "bundle-analyze": "cross-env BUNDLE_ANALYZE=true npm start",
    "start": "npm run build && cross-env NODE_ENV=production node ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sqrtox/sqrtox-website.git"
  },
  "keywords": [
    "sqrtox"
  ],
  "author": "sqrtox",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/sqrtox/sqrtox-website/issues"
  },
  "homepage": "https://github.com/sqrtox/sqrtox-website#readme",
  "devDependencies": {
    "@next/bundle-analyzer": "^12.3.0",
    "@next/eslint-plugin-next": "^12.3.0",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.14",
    "@types/gtag.js": "^0.0.11",
    "@types/react-dom": "^18.0.6",
    "@types/redux-logger": "^3.0.9",
    "@types/statuses": "^2.0.0",
    "@types/uglify-js": "^3.17.0",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.23.1",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-sort-exports": "^0.7.0",
    "npm-run-all": "^4.1.5",
    "redux-logger": "^3.0.6",
    "rimraf": "^3.0.2",
    "ts-toolbelt": "^9.6.0",
    "tsc-alias": "^1.7.0",
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@mui/icons-material": "^5.10.3",
    "@mui/material": "^5.10.5",
    "@next/env": "^12.3.0",
    "@react-hook/size": "^2.1.2",
    "@reduxjs/toolkit": "^1.8.5",
    "axios": "^0.27.2",
    "bcrypt": "^5.0.1",
    "canvas": "^2.10.1",
    "content-type-parser": "^1.0.2",
    "cookie": "^0.5.0",
    "dayjs": "^1.11.5",
    "deta": "^1.1.0",
    "express": "^4.18.1",
    "gray-matter": "^4.0.3",
    "localforage": "^1.10.0",
    "next": "^12.3.0",
    "next-pwa": "^5.6.0",
    "next-seo": "^5.5.0",
    "next-sitemap": "^3.1.22",
    "prism-themes": "^1.9.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.2",
